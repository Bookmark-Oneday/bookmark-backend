{
  "name": "backend_template",
  "version": "1.0.0",
  "description": "NodeJS backend template",
  "main": "app.js",
  "scripts": {
    "test": "jest --forceExit --detectOpenHandles",
    "test:dao": "jest --forceExit --detectOpenHandles ./src/dao/",
    "test:dao:repositories": "jest --forceExit --detectOpenHandles ./src/dao/repositories/",
    "test:dao:connections": "jest --forceExit --detectOpenHandles ./src/dao/connections/",
    "test:controllers": "jest --forceExit --detectOpenHandles ./src/controllers/",
    "test:routes": "jest --forceExit --detectOpenHandles ./src/routes/",
    "test:services": "jest --forceExit --detectOpenHandles ./src/services/",
    "start:local": "APP_ENV=local nodemon ./src/app.js",
    "start:dev": "APP_ENV=dev node ./src/app.js",
    "start:prod": "APP_ENV=prod node ./src/app.js",
    "lint": "eslint ./src"
  },
  "repository": {
    "type": "git",
